<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.paichi.modules.effect.mapper.EffectMapper">

    <select id="getEffect" parameterType="String" resultType="Effect">
        select * from EFFECT where EFFECT_NAME = #{effectName}
    </select>

    <insert id="saveEffect" parameterType="Effect">
        <selectKey  keyProperty="effectId" resultType="Integer" order="BEFORE">
            select EFFECT_ID_SEQ.nextval from dual
        </selectKey>
        insert into EFFECT (EFFECT_ID, EFFECT_NAME) values(#{effectId}, #{effectName})
    </insert>
</mapper>
